<simple-objects xmlns="http://tail-f.com/ns/simple">
  <max-number-of-servers>10</max-number-of-servers>
  <max-number-of-hosts>10</max-number-of-hosts>
  <hosts>
    <host>
      <name>saturn@tail-f.com</name>
      <enabled>true</enabled>
      <servers>
        <server>
          <name>ftp</name>
          <address>
            <ip>192.168.0.1</ip>
            <port>5001</port>
          </address>
        </server>
        <server>
          <name>www</name>
          <address>
            <ip>192.168.0.1</ip>
            <port>5000</port>
          </address>
        </server>
      </servers>
    </host>
    <host>
      <name>vega@tail-f.com</name>
      <enabled>false</enabled>
      <servers/>
    </host>
  </hosts>
  <services>
    <service>
      <address>192.168.0.1</address>
      <port-number>5000</port-number>
      <host-name>saturn@tail-f.com</host-name>
      <server-name>www</server-name>
    </service>
    <service>
      <address>192.168.0.1</address>
      <port-number>5001</port-number>
      <host-name>saturn@tail-f.com</host-name>
      <server-name>ftp</server-name>
      <radius-ip>10.0.0.1</radius-ip>
    </service>
  </services>
</simple-objects>
