<!-- -*- nxml -*- -->
<webspec xmlns="http://tail-f.com/ns/webspec/1.0"
	 targetNamespace="http://tail-f.com/ns/example/syslog">
  <!-- Element settings -->

  <options>
    <unrollDynamicElements/>
  </options>

  <elemSettings tagpath="/syslog/message/line">
    <graph>none</graph>
  </elemSettings>
  <elemSettings tagpath="/syslog/message/text">
    <size>120</size>
  </elemSettings>
</webspec>
